
DEBOUNCE = 200

SEN button pin=11
ACT led2
    pin = 10 LOW

ACT led
    pin=9 LOW

WHEN (button.pin HIGH AND (led.pin LOW AND led2.pin LOW)) {
    led.pin = !led.pin
}

WHEN (button.pin HIGH AND led.pin HIGH){
    led.pin = !led.pin
    led2.pin = !led2.pin
}

WHEN (button.pin HIGH AND led2.pin HIGH){
    led2.pin = !led2.pin
}